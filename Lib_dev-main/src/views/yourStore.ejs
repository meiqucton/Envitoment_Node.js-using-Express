<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Your store</title>
    <link href="/css/yourStore.css" type="text/css" rel="stylesheet">
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.0.0-beta3/css/all.min.css">
    <link href="/css/notifacation.css " type="text/css" rel="stylesheet">

</head>
<body>
    <% if (typeof messages !== 'undefined' && messages.error) { %>
        <div class="notification error-notification">
            <%= messages.error %>
        </div>
    <% } %>
    <% if (typeof messages !== 'undefined' && messages.success) { %>
        <div class="notification success-notification">
            <%= messages.success %>
        </div>
    <% } %>
    
    <div class="product-container">

    <form action="/CreteVoucher" method="get">
        <button type="submit"><i class="fa-solid fa-ticket"></i></button>
    </form>
    <form action="/addProduct" method = "get">
        <button class="addProduct-button" type="submit"><i class="fa-solid fa-cart-plus"></i></button> 
    </form>

    <form action="/wareHouses" method = "get">
        <button class="warehouse-button"type="submit"><i class="fa-solid fa-warehouse"></i> </button> 
    </form>
    <form action="/consulting" method = "get">
        <button class="in4-button" type="submit"><i class="fa-regular fa-comments"></i></button> 
    </form>

    </div>  
</body>
</html>